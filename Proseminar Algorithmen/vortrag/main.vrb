\frametitle{Vergleichender Baustein (ii)}
\begin{lstlisting}[laguage={inform},tabsize=4]
    void comp(chan in1, in2, out1 out2){
        a = <- in1;
        b = <- in2;

        if (a < b){
            out1 <- a;
            out2 <- b;
            return void;
        }
        out1 <- b;
        out2  <- a;
        return void;
    }
\end{lstlisting}
